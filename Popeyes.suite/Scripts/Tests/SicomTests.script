//Sicom Test Case 1: 12pc bone-in wings ala-carte
Common.launchSicomPOS
Common.setPlatformName "POS"
Common.setPOSPlatformName "Sicom"
Common.setTestCaseName "12P Wings ALC"
//POS
"DAIWrappers/POSSicom".selectDiningOption
"DAIWrappers/POSSicom".selectMealAndOption
"DAIWrappers/POSSicom".selectMainCourse
"DAIWrappers/POSSicom".selectSide
"DAIWrappers/POSSicom".selectDrink
"DAIWrappers/POSSicom".selectFlavors
"DAIWrappers/POSSicom".proceedToTender
"DAIWrappers/POSSicom".exactPayment

//KDS
"DAIWrappers/KDS".connectToKDS
"DAIWrappers/KDS".bumpOrderIDOnKDS
"DAIWrappers/KDS".disconnectFromKDS

//Receipt
"DAIWrappers/POSSicom".validateSicomReceipt

